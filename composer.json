{
    "name": "notyourtechguy/birdpay",
    "description": "Paymaya integration with laravel 5.6",
    "license": "MIT",
    "authors": [
        {
            "name": "Arki Valencia II",
            "email": "arkivalenciaii@notyourtechguy.me"
        }
    ],
    "autoload": {
        "psr-4": {
            "Notyourtechguy\\Birdpay\\": "src/",
            "Notyourtechguy\\Birdpay\\Test\\": "tests/"
        }
    },
    "extra": {
